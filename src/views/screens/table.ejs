<section class="row justify-content-center pt-5">
    <table class="table table-hover ">
        <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">link</th>
                <th scope="col">date</th>
            </tr>
        </thead>
        <tbody id="userTBody">
            <% screenList.forEach(function(screen, i){ %>
            <tr>
                <th scope="row"><%=i + 1%></th>
                <td id="email-date"><a href="<%=screen.link %>"><%=screen.link %></a></td>
                <td id="email-count"><%=screen.date %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</section>
</div>
